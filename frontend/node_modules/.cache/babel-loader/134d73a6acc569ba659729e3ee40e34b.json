A,SAdA,uBAcA;AAAA;;AACA,WAAA,YAAA,GAAA,KAAA;;AACA,UAAA,KAAA,SAAA,CAAA,IAAA,IAAA,KAAA,SAAA,CAAA,IAAA,EAAA;AACA;AACA,QAAA,YAAA,CAAA,KAAA,SAAA,CAAA,CAAA,IAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,IAAA,CAAA,KAAA,CAAA,SAAA;AACA,SAHA;AAIA,OANA,MAMA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA,KAzBA;;AA0BA;AACA;AACA;AACA;AACA;AACA,IAAA,UA/BA,sBA+BA,KA/BA,EA+BA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,QAAA,GAAA,KAAA,SAAA,CAAA,KAAA,EAAA,IAAA;AACA,KAlCA;;AAmCA;AACA;AACA;AACA,IAAA,WAtCA,yBAsCA;AACA,WAAA,SAAA,CAAA,KAAA,WAAA,EAAA,IAAA,GAAA,KAAA,QAAA;AACA,WAAA,WAAA,GAAA,CAAA,CAAA;AACA,KAzCA;;AA0CA;AACA;AACA;AACA;AACA,IAAA,YA9CA,wBA8CA,KA9CA,EA8CA;AAAA;;AACA,WAAA,QAAA,CAAA,SAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,OANA;AAOA;AAtDA;AArBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"add-button\">\r\n      <el-button type=\"primary\" @click=\"showEditForm = true\">\r\n        添加成员\r\n      </el-button>\r\n    </div>\r\n    <el-table\r\n      :data=\"groupList\"\r\n      element-loading-text=\"Loading\"\r\n      :row-class-name=\"tableRowClassName\"\r\n      border\r\n      fit\r\n    >\r\n      <!-- 序号 -->\r\n      <el-table-column align=\"center\" label=\"序号\" width=\"50\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.$index + 1 }}\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <!-- 用户名 -->\r\n      <el-table-column align=\"center\" label=\"用户名\" width=\"300\">\r\n        <template slot-scope=\"scope\">\r\n          <template v-if=\"scope.$index == editCurrent\">\r\n            <el-input v-model=\"editName\" size=\"small\" />\r\n          </template>\r\n          <span v-else> {{ scope.row.name }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 角色 -->\r\n      <!--TODO 打算使用下拉选项 -->\r\n      <el-table-column align=\"center\" label=\"角色\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.role }}\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 操作 -->\r\n      <el-table-column label=\"操作\" width=\"210\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            v-if=\"scope.$index == editCurrent\"\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            @click=\"confirmEdit\"\r\n          >\r\n            确定\r\n          </el-button>\r\n          <el-button\r\n            v-else\r\n            size=\"mini\"\r\n            type=\"success\"\r\n            @click=\"handleEdit(scope.$index)\"\r\n          >\r\n            编辑\r\n          </el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete(scope.$index)\"\r\n          >\r\n            删除\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 编辑框 -->\r\n    <div>\r\n      <el-dialog :visible.sync=\"showEditForm\" :width=\"width\">\r\n        <el-form label-position=\"left\" label-width=\"80px\" :model=\"newMember\">\r\n          <el-form-item label=\"成员昵称\">\r\n            <el-input v-model=\"newMember.name\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"成员角色\">\r\n            <el-input v-model=\"newMember.role\" />\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button type=\"primary\" @click=\"addMember()\">\r\n            确定\r\n          </el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { addMemberApi } from \"@/api/group\";\r\nimport { mapGetters } from \"vuex\";\r\nexport default {\r\n  name: \"Group\",\r\n\r\n  data() {\r\n    return {\r\n      newMember: {}, // 添加的成员信息\r\n      showEditForm: false, // 显示编辑对话框\r\n      editName: \"\", // 编辑的用户名\r\n      editCurrent: -1, // 编辑的行\r\n    };\r\n  },\r\n  computed: {\r\n    // 编辑框宽度\r\n    width() {\r\n      return window.innerWidth <= 400 ? \"80%\" : \"30%\";\r\n    },\r\n    ...mapGetters([\"name\", \"roles\"]),\r\n    groupList() {\r\n      return [{ name: this.name, role: this.roles[0] }];\r\n    },\r\n  },\r\n  methods: {\r\n    /**\r\n     * 表格样式\r\n     */\r\n    // eslint-disable-next-line no-unused-vars\r\n    tableRowClassName({ row, rowIndex }) {\r\n      if (rowIndex % 2) {\r\n        return \"highlight-row\";\r\n      }\r\n      return \"\";\r\n    },\r\n    /**\r\n     * 添加成员\r\n     */\r\n    addMember() {\r\n      this.showEditForm = false;\r\n      if (this.newMember.name && this.newMember.role) {\r\n        // 发送新成员信息\r\n        addMemberApi(this.newMember).then(() => {\r\n          this.$message.success(\"添加成功\");\r\n          this.groupList.push(this.newMember);\r\n        });\r\n      }else{\r\n        this.$message.error(\"请将成员信息填写完整\")\r\n      }\r\n    },\r\n    /**\r\n     * 编辑成员信息\r\n     * @param {int} index 索引\r\n     * @param {Object} row 当前行\r\n     */\r\n    handleEdit(index) {\r\n      this.editCurrent = index;\r\n      this.editName = this.groupList[index].name;\r\n    },\r\n    /**\r\n     * 确认编辑\r\n     */\r\n    confirmEdit() {\r\n      this.groupList[this.editCurrent].name = this.editName;\r\n      this.editCurrent = -1;\r\n    },\r\n    /**\r\n     * 删除成员\r\n     * @param {int} index 索引\r\n     */\r\n    handleDelete(index) {\r\n      this.$confirm(\"确定要删除吗?\", \"警告\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      }).then(() => {\r\n        this.groupList.splice(index, 1);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\">\r\n.add-button {\r\n  margin: 20px 20px;\r\n}\r\n.el-table .highlight-row {\r\n  background: #ebf0fa;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/groupManage"}]}