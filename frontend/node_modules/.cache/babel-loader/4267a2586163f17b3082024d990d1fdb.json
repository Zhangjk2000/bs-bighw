A;AAmBA,IAAA,WAnBA,yBAmBA;AACA,MAAA,eAAA,CAAA,KAAA,SAAA,CAAA;AACA;AArBA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div class=\"form\">\r\n    <div>\r\n      <div><strong>标签名:</strong></div>\r\n      <input v-model=\"labelInfo.text\" type=\"text\" />\r\n    </div>\r\n    <div>\r\n      <div>\r\n        <strong>快捷键(输入一个字母或者数字,不输入代表不使用快捷键标注):</strong>\r\n      </div>\r\n      <input v-model=\"labelInfo.shortcut\" type=\"text\" />\r\n    </div>\r\n    <div>\r\n      <div>\r\n        <strong>标签颜色:</strong>\r\n      </div>\r\n      <el-color-picker\r\n        v-model=\"labelInfo.color\"\r\n        show-alpha\r\n        :predefine=\"$store.state.errorAnalysis.colorArray\"\r\n      />\r\n    </div>\r\n    <div class=\"button-group\">\r\n      <el-button type=\"warning\" @click=\"cancel\">\r\n        取消\r\n      </el-button>\r\n      <el-button type=\"primary\" @click=\"addOrUpdateLabel\">\r\n        确定\r\n      </el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { addLabelsApi, updateLabelsApi } from \"@/api/annotateData\";\r\nexport default {\r\n  props: {\r\n    isAddLabel: {\r\n      type: Boolean,\r\n      default: true,\r\n    },\r\n    labelInfo: {\r\n      type: Object,\r\n      default() {\r\n        return { text: \"\", color: \"#ff0000\", shortcut: \"\" };\r\n      },\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    cancel() {\r\n      this.labelInfo.text = \"\";\r\n      this.labelInfo.shortcut = \"\";\r\n      this.$emit(\"closeDialog\");\r\n    },\r\n    addOrUpdateLabel() {\r\n      if (this.isAddLabel) {\r\n        this.addLabel();\r\n      } else {\r\n        this.updateLabel();\r\n      }\r\n      this.$emit(\"closeDialog\");\r\n    },\r\n    addLabel() {\r\n      addLabelsApi(this.labelInfo).then((res) => {\r\n        this.$store.dispatch(\"annotate/setLabels\", res.data.labels);\r\n      });\r\n    },\r\n    updateLabel() {\r\n      updateLabelsApi(this.labelInfo);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.form {\r\n  div {\r\n    margin-top: 10px;\r\n    input {\r\n      margin-top: 10px;\r\n      outline: none;\r\n      width: 100%;\r\n      border: none;\r\n      border-bottom-style: solid;\r\n      height: 30px;\r\n    }\r\n  }\r\n  .button-group {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/labelsManage/components"}]}